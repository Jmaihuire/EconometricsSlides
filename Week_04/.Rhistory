tib <- tibble(U1 = rnorm(1000), A = rnorm(1000), B = rnorm(1000)) %>%
mutate(C = U1 + rnorm(1000), D = U1 + rnorm(1000)) %>%
mutate(X = A + C + rnorm(1000)) %>%
mutate(Y = 4*X + A + B + D + rnorm(1000))
set.seed(1234)
library(tidyverse)
tib <- tibble(U1 = rnorm(1000), A = rnorm(1000), B = rnorm(1000)) %>%
mutate(C = U1 + rnorm(1000), D = U1 + rnorm(1000)) %>%
mutate(X = A + C + rnorm(1000)) %>%
mutate(Y = 4*X + A + B + D + rnorm(1000))
lm(Y~X, data = tib)
lm(Y~X, data = tib)$Coefficients
lm(Y~X, data = tib)$coefficients
dag <- dagify(Y~X+A+B+D,
X~A+C,
C~U1,
D~U1,
coords=list(
x=c(X = 1, Y = 3, A = 2, B = 3, D = 2.5, C = 1, U1 = 1.5),
y=c(X = 1, Y = 1, A = 2, B = 2, D = 2.5, C = 2.5, U1 = 3)
)) %>% tidy_dagitty()
dag <- dagify(Y~X+A+B+D,
X~A+C,
C~U1,
D~U1,
coords=list(
x=c(X = 1, Y = 3, A = 2, B = 3, D = 2.5, C = 1, U1 = 1.5),
y=c(X = 1, Y = 1, A = 2, B = 2, D = 2.5, C = 2.5, U1 = 3)
)) %>% tidy_dagitty()
ggdag_classic(dag,node_size=10) +
theme_void() +
expand_limits(x=c(.5,3.5))
m1 <- lm(Y~X, data = tib)
m1$coefficients
m2 <- lm(Y~X + A + C, data = tib)
m3 <- lm(Y~X + A + D, data = tib)
export_summs(m1,m2,m3)
